    </div>
    <div class="form-group">
        <label asp-for="gobernador_asesores.Telefono"></label>
        <input asp-for="gobernador_asesores.Telefono" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="gobernador_asesores.Rol"></label>
        <input asp-for="gobernador_asesores.Rol" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="gobernador_asesores.Genero"></label>
        <input asp-for="gobernador_asesores.Genero" class="form-control">
