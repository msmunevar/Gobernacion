        <input asp-for="gobernador_asesores.Oficinas_Visitadas" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="gobernador_asesores.Ubicacion"></label>
        <input asp-for="gobernador_asesores.Ubicacion" class="form-control">
