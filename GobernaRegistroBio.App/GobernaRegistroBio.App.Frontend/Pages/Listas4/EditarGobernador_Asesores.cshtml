    <div class="form-group">
        <label asp-for="gobernador_asesores.Nombre"></label>
        <input asp-for="gobernador_asesores.Nombre" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="gobernador_asesores.Apellidos"></label>
        <input asp-for="gobernador_asesores.Apellidos" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="gobernador_asesores.Identificacion"></label>
        <input asp-for="gobernador_asesores.Identificacion" class="form-control">
